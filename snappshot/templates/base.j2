<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:py="http://genshi.edgewall.org/"
      xmlns:xi="http://www.w3.org/2001/XInclude">
  <head>
	{% block starthead %}{% endblock starthead %}
    <meta charset="{{ resource.meta.charset }}" />
    <title>{% block title %}Snappshot{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="{{ resource.meta.description }}" />
    <meta name="author" content="{{ resource.meta.author }}" />

    <!-- Le styles -->
    {# <link href="{{ STATIC_URL }}css/bootstrap.min.css" rel="stylesheet"> #}
    <link href="{{ STATIC_URL }}css/readable-bootstrap.min.css" rel="stylesheet">
	{# <link href="{{ STATIC_URL }}css/united-bootstrap_dark.min.css" rel="stylesheet" /> #}
	<link rel="stylesheet" href="{{ STATIC_URL }}css/font-awesome.min.css" />
	
  <link href="{{ STATIC_URL }}css/bootstrap-fileupload.min.css" rel="stylesheet">
  
  <link rel="stylesheet" href="{{ STATIC_URL }}css/custom_bootstrap.css" />
  

    <style type="text/css">
      body {
        padding-top: 80px;
        padding-bottom: 40px;
      }
      .sidebar-nav {
        padding: 9px 0;
      }
      /*.navbar .nav li a, .navbar .brand {
        color: #F15700;
  -webkit-mask-image: -webkit-gradient(linear, left top, 
    left bottom, from(rgba(0,0,0,.3)), to(rgba(0,0,0,1)));
      }*/

      .navbar .nav li  {
        padding-left: 0px;
        padding-right: 0px;
      }

      @media (max-width: 980px) {
        /* Enable use of floated navbar text */
        .navbar-text.pull-right {
          float: none;
          padding-left: 5px;
          padding-right: 5px;
        }
      }
    </style>
    <link href="{{ STATIC_URL }}css/bootstrap-responsive.min.css " rel="stylesheet" />
    

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="{{ STATIC_URL }}js/html5shiv.js"></script>
    <![endif]-->



  </head>

  <body>

    <div class="navbar  navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container-fluid">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="/snappshot">Snappshot</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
            <!-- Put nav items here -->
            </ul>
            <ul class="nav pull-right inline">
            <li><a href="https://github.com/JustinJudd/snappshot"><i class="icon-github"></i></a></li>
            <li><a href="/snappshot/faq"><i class="icon-question-sign"></i></a></li>
            
            </ul>
     
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

  <div class="container-fluid">
  <div class="row-fluid">
		{% block content %}
			{% block main %}
			{% endblock main %}
	  {% endblock content %}
      </div><!--/row-->

      {% block backgrounds %}
      {% endblock backgrounds %}



      <hr />

      <footer class="text-center">
        <p>Created 2013 by <a href="http://www.justinjudd.org">Justin Judd <i class="icon-heart"></i></a></p>
      </footer>

    </div><!--/.fluid-container-->

    <!-- Placed at the end of the document so the pages load faster -->
    <script src="{{ STATIC_URL }}js/libs/jquery-1.9.1.min.js"></script>
    <script src="{{ STATIC_URL }}js/bootstrap.min.js"></script>
    <script src="{{ STATIC_URL }}js/bootstrap-fileupload.min.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}js/bootstrap-filestyle.min.js"></script>



	{% block analytics %}
	{# {% include "analytics.j2" %} #}
	{% endblock analytics %}


  </body>
</html>

